{# templates/registration/inscription.html.twig #}

{% extends 'base.html.twig' %}

{% block title %}Inscription{% endblock %}

{% block body %}
    <h1>Inscription</h1>

    {{ form_start(form) }}
    {{ form_row(form.email) }}
    {{ form_row(form.password) }}
    {{ form_row(form.nom) }}
    {{ form_row(form.prenom) }}
    {{ form_row(form.siret) }}
    {{ form_row(form.entreprise) }}
    {{ form_row(form.reference_client) }}
    {{ form_row(form.coefficient) }}
    {{ form_row(form.telephone) }}
    {{ form_row(form.type_client) }}
    {{ form_row(form.id_adresse_facturation) }}
    {{ form_row(form.id_adresse_livraison) }}
    {{ form_row(form.id_commercial) }}

    <button type="submit" class="btn btn-primary">S'inscrire</button>
    {{ form_end(form) }}
{% endblock %}
